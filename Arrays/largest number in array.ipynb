   "source": [
    "# largest number in array\n",
    "a=int(input())\n",
    "b=list()\n",
    "for i in range(a):\n",
    "    x=int(input())\n",
    "    b.append(x)\n",
    "print(b)\n",
    "i=1\n",
    "res=0\n",
    "for i in range(a):\n",
    "    if (b[i]> b[res]):\n",
    "        res=i\n",
    "    i+=1\n",
    "print(res)"
