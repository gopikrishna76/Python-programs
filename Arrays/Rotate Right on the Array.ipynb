   "source": [
    "\n",
    "a = int(input(  \"Enter the size of an array : \"))\n",
    "b = list()\n",
    "for i in range(a):\n",
    "    x = int(input(\"Enter the next value : \"))\n",
    "    b.append(x)\n",
    "print(b)\n",
    "\n",
    "k =int(input())\n",
    "def rotate(b, k):\n",
    "    k = k % len(b)\n",
    "    b[:] = b[k:] + b[:k]\n",
    "    return b\n",
    "rotate(b,k)"
